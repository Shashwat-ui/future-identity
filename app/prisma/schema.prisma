datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model QRToken {
  id        String   @id @default(cuid())
  payload   Json
  expiresAt DateTime
  revoked   Boolean  @default(false)
  createdAt DateTime @default(now())
}